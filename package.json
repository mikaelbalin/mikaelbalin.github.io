{
  "name": "mikhailbalin.github.io",
  "version": "1.0.0",
  "description": "A personal website and blog built with Payload CMS and Next.js.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "playwright install",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "dev:db": "pnpm db:up && wait-on tcp:5432 && pnpm dev",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:analyze": "ANALYZE=true pnpm build",
    "ci": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload migrate && pnpm build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "check-types": "tsc --noEmit",
    "check-types:watch": "pnpm check-types --watch",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test-storybook": "vitest --project=storybook",
    "test-storybook:ui": "vitest --project=storybook --ui",
    "test-storybook:coverage": "vitest run --project=storybook --coverage",
    "test:unit": "vitest --project=unit",
    "test:unit:ui": "vitest --project=unit --ui",
    "test:unit:coverage": "vitest run --project=unit --coverage",
    "test": "pnpm test-storybook run && pnpm test:unit run",
    "backup": "./backup.sh"
  },
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.6.1",
    "@hookform/resolvers": "5.2.2",
    "@kaelui/hooks": "0.3.2",
